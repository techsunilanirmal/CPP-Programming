//Print all natural number from 1 to N:-

#include<iostream>
using namespace std;

int main()
{
    int num,i;
    
    cout<<"Enter any number:";
    cin>>num;
    
    cout<<"Natural number from 1 to "<<num<<" is given below"<<endl;
    
    for(i=1;i<=num;i++)
    {
        cout<<i<<" ";
    }
    
    return 0;
}


//Print all natural number from 1 to N in reverse order:-

#include<iostream>
using namespace std;

int main()
{
    int num,i;
    
    cout<<"Enter any number:";
    cin>>num;
    
    cout<<"Natural number from 1 to "<<num<<" is given below"<<endl;
    
    for(i=num;i>=1;i--)
    {
        cout<<i<<" ";
    }
    
    return 0;
}


//print sum of natural number from 1 to N:-

#include<iostream>
using namespace std;

int main()
{
    int num,i,sum=0;
    
    cout<<"Enter any number:";
    cin>>num;
    
    cout<<"Natural number from 1 to "<<num<<" is given below:-"<<endl;
    
    for(i=1;i<=num;i++)
    {
        cout<<i<<" ";
        sum=sum+i;
    }
    cout<<"\nTotal sum of natural numbers are:"<<sum;
    
    return 0;
}


//print A to Z:-

#include<iostream>
using namespace std;

int main()
{
    char ch;
    
    for(ch='A';ch<='Z';ch++)
    {
        cout<<ch<<" ";
    }
    
    return 0;
}


//Find factorial of any number:

#include<iostream>
using namespace std;

int main()
{
    int num,i,fact=1;
    
    cout<<"Enter any number:";
    cin>>num;
    
    for(i=1;i<=num;i++)
    {
        fact=fact*i;
    }
    
    cout<<"Factorial is:"<<fact;
    
    return 0;
}



//Find factor of any number:

#include<iostream>
using namespace std;

int main()
{
    int num,i;
    
    cout<<"Enter any number:";
    cin>>num;
    
    cout<<"Factor is given below:"<<endl;
    
    for(i=1;i<=num;i++)
    {
        if(num%i==0)
        {
            cout<<i<<" ";
        }
    }
    
    return 0;
}


//Find power of any number:

#include<iostream>
using namespace std;

int main()
{
    int base,pow,i,f=1;
    
    cout<<"Enter base:";
    cin>>base;
    
    cout<<"Enter power:";
    cin>>pow;
    
    for(i=1;i<=pow;i++)
    {
        f=f*base;
    }
    
    cout<<"Result is:"<<f;
    
    return 0;
}


//Fibonacci Series:

#include<iostream>
using namespace std;

int main()
{
    int num,a,i,b=1,c=0;
    
    cout<<"Enter any number upto you want to print fibonacci series:";
    cin>>num;
    
    for(i=1;i<=num;i++)
    {
        cout<<c<<" ";
        
        a=b;
        b=c;
        
        c=a+b;
    }
    
    return 0;
}
