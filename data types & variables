//Data types and variables:-


#include<iostream>
using namespace std;

int main()
{
    int a=10;
    char b='S';
    float c=3.14;
    
    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    
    
    return 0;
}

//  I/O function:-

#include<iostream>
using namespace std;

int main()
{
   cout<<"Hello world!!!"<<endl;
   
   int a;
   
   cout<<"Enter a number:";
   cin>>a;
    
    
    return 0;
}
